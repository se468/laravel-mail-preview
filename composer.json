{
    "name": "themsaid/laravel-mail-preview",
    "description": "Adds a 'preview' mail driver for testing emails in laravel.",
    "keywords": ["laravel", "mail"],
    "homepage": "https://github.com/themsaid/laravel-preview-mail-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "theMohamedSaid@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1",
        "illuminate/mail": "~5.1",
        "illuminate/filesystem": "~5.1",
        "mockery/mockery": "~0.9.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\MailPreview\\": "src"
        },
        "classmap": [
            "tests"
        ]
    }
}
